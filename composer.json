{
  "name": "neogroup/neophp",
  "description": "The NeoPHP Framework.",
  "keywords": ["framework", "neo", "php"],
  "license": "Apache",
  "homepage": "https://github.com/luismanuelamengual/NeoPHP",
  "authors":
  [
    {
      "name": "Luis Manuel Amengual",
      "email": "luismanuelamengual@gmail.com"
    }
