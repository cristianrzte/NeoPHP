{
  "name": "neogroup/neophp",
  "description": "The NeoPHP Framework.",
  "keywords": ["framework", "neo", "php"],
  "license": "MIT",
  "homepage": "https://github.com/luismanuelamengual/NeoPHP",
  "authors":
  [
    {
      "name": "Luis Manuel Amengual",
      "email": "luismanuelamengual@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "filp/whoops": "^2.1.14",
    "theorchard/monolog-cascade": "^0.5.0",
    "php-curl-class/php-curl-class": "^8.0",
    "phpmailer/phpmailer": "^6.0",
    "vlucas/phpdotenv": "^3.3",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "files": [
      "src/NeoPHP/helpers.php"
    ],
    "psr-4": { "": "src/" }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
